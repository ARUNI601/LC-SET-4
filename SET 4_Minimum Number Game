class Solution(object):
    def numberGame(self, nums):
        l=(len(nums))/2
        arr=[]
        for i in range(l):
            alice=min(nums)
            nums.remove(alice)
            bob=min(nums)
            nums.remove(bob)
            arr.append(bob)
            arr.append(alice)
        return arr
solution=Solution()


        
